        "await shellx('xdotool key \"ctrl+a\"; for _ in {1..10}; do xdotool key --delay 0 \"shift+Up\"; done; xdotool key \"ctrl+c\"')",
        "await shellx('xclip -o | tr -d \"\\n\" | node -e \"console.log(encodeURIComponent(require(\\'fs\\').readFileSync(0, \\{encoding: \\'utf8\\'})))\" | xargs -I{} firefox -P clone3 -new-tab -url \"https://www.google.com/search?q={}\"; xdotool key \"alt+o\"; xdotool key \"ctrl+9\" \"alt+9\"')",
